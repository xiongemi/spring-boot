      "executor": "@nx/gradle:gradle",
      "options": {
        "excludeDependsOn": false
      }
    },
    "antora": {
      "executor": "@nx/gradle:gradle",
      "options": {
        "excludeDependsOn": false
      }
    },
    "antoraNpmInstall": {
      "executor": "@nx/gradle:gradle",
      "options": {
        "excludeDependsOn": false
      }
    }
  },
  "bustCache": 14,
  "nxCloudUrl": "https://snapshot.nx.app",
  "nxCloudId": "67dad8ae45125407e384edab"
}
